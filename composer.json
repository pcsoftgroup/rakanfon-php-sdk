{
    "name": "pcsoftgroup/rakanfon-php-sdk",
    "description": "The official RakanFon PHP SDK.",
    "keywords": [
        "rakanfon",
        "sdk"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/pcsoftgroup/rakanfon-php-sdk/issues",
        "source": "https://github.com/pcsoftgroup/rakanfon-php-sdk"
    },
    "authors": [
        {
            "name": "Huthaifah Mustafa",
            "email": "hsul4n@pcsoftgroup.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "illuminate/contracts": "^8.83",
        "illuminate/http": "^8.83",
        "illuminate/support": "^8.83",
        "mtownsend/xml-to-array": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.4|^9.0"
    },
    "autoload": {
        "psr-4": {
            "PCsoft\\RakanFon\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
